{
    "Four Column Layout Example Styles": {
        "prefix": "ld-gc-se-four-column-layout-styles",
        "body": [
            ".container {",
            "\tbackground: #FFF;",
            "\theight: 100%;",
            "\tbox-sizing: border-box;",
            "}",
            "",
            ".column {",
            "\tfont-size: 1.6rem;",
            "\tpadding: 20px;",
            "\tbackground: #0048AA;",
            "\tcolor: #FFF;",
            "\tborder: 2px solid #EEE;",
            "}"
        ]
    },

    "Side Bar Example Styles": {
        "prefix": "ld-gc-se-side-bar-styles",
        "body": [
            "main {",
            "\theight: 100%;",
            "}",
            "",
            "aside {",
            "\tbackground: #0048AA;",
            "\tcolor: #FFF;",
            "\tfont-size: 1.6rem;",
            "}",
            "",
            "section {",
            "\tbackground: #FFF;",
            "}",
            "",
            "aside, section {",
            "\tpadding: 20px;",
            "}",
            "",
            "h1 {",
            "\tfont-size: 2rem;",
            "}",
            "",
            "p {",
            "\tfont-size: 1.6rem;",
            "\tmargin-top: 1.4rem;",
            "}"
        ]
    },

    "Centering A Modal Dialog Example Styles": {
        "prefix": "ld-gc-se-centering-modal-dialog-styles",
        "body": [
            ".overlay {",
            "\tposition: fixed;",
            "\ttop: 0;",
            "\tleft: 0;",
            "\tbottom: 0;",
            "\tright: 0;",
            "\tbackground: rgba(0,0,0, 0.3);",
            "}",
            "",
            ".modal-container {",
            "\tbackground: #FFF;",
            "\twidth: 400px;",
            "\theight: 300px;",
            "\tpadding: 20px;",
            "\tbox-sizing: border-box;",
            "}",
            "",
            ".modal-content {",
            "\ttext-align: center;",
            "}",
            "",
            "h1 {",
            "\tfont-size: 2rem;",
            "}",
            "",
            "p {",
            "\tfont-size: 1.6rem;",
            "\tmargin-top: 1.4rem;",
            "}"
        ]
    },

    "Sticky Footer Example Styles": {
        "prefix": "ld-gc-se-sticky-footer-styles",
        "body": [
            "header, footer, main {",
            "\tpadding: 20px;",
            "}",
            "",
            "main {",
            "\tbackground: #FFF;",
            "}",
            "",
            "header, footer {",
            "\tbackground: #0048AA;",
            "}",
            "",
            "header h1, footer h2 {",
            "\tcolor: #FFF;",
            "}"
        ]
    },

    "Formatted Form Example Styles": {
        "prefix": "ld-gc-se-formatted-form-styles",
        "body": [
            "form {",
            "\twidth: 600px;",
            "\tpadding: 20px;",
            "\tborder-radius: 5px;",
            "\tborder: 1px solid #444;",
            "}",
            "",
            "h3 {",
            "\tcolor: #0048AA;",
            "\ttext-align: center;",
            "\tmargin-bottom: 0.8rem;",
            "}",
            "",
            "fieldset {",
            "\tbackground: #EEE;",
            "\tpadding: 8px 10px;",
            "\tborder: none;",
            "}",
            "",
            "label {",
            "\tcursor: pointer;",
            "}",
            "",
            "input, textarea {",
            "\tpadding: 5px 10px;",
            "\tfont-size: 1.5rem;",
            "\tborder-radius: 3px;",
            "\tborder: 1px solid rgba(25, 25, 25, 0.4);",
            "}",
            "",
            "button {",
            "\tpadding: 6px;",
            "\tfont-size: 1.5rem;",
            "\tbackground: #246fa8;",
            "\tcolor: #FFF;",
            "\tborder: 1px solid #57c1eb;",
            "\tborder-radius: 5px;",
            "\tmargin-top: 5px;",
            "}"
        ]
    }
}