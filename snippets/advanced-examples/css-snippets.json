{
    "Holy Grail Example Basic Styles": {
        "prefix": "ld-gc-ae-holy-grail-styles",
        "body": [
            "h1 {",
            "\tfont-size: 1.6rem;",
            "\tfont-weight: normal;",
            "}",
            "",
            "#holygrail-container {",
            "\tbackground: #FFF;",
            "\theight: 100%;",
            "\tmin-width: 400px;",
            "\tmin-height: 270px;",
            "}",
            "",
            "header, footer, nav, aside, section {",
            "\tpadding: 20px;",
            "\tborder: 1px solid #000;",
            "\ttext-align: center;",
            "}"
        ]
    },

    "Media Objects Layout Basic Styles": {
        "prefix": "ld-gc-ae-media-objects-layout-styles",
        "body": [
            ".media {",
            "\tbackground: #0048AA;",
            "\tcolor: #FFF;",
            "\tpadding: 15px;",
            "\twidth: 100%;",
            "\tmax-width: 600px;",
            "\tmargin-bottom: 40px;",
            "\tbox-sizing: border-box;",
            "}",
            "",
            ".media img {",
            "\tborder-radius: 100%;",
            "\twidth: 48px;",
            "\theight: 48px;",
            "}",
            "",
            ".media h2 {",
            "\tfont-size: 1.6rem;",
            "\tmargin-bottom: 1.3rem;",
            "\tcolor: #FFF;",
            "}",
            "",
            ".media section {",
            "\tfont-size: 1.3rem;",
            "}",
            "",
            ".media section p {",
            "\tmargin-bottom: 0.6rem;",
            "}",
            "",
            ".comment {",
            "\tbackground: #CCDDFF;",
            "\tpadding: 7px;",
            "\tcolor: #131;",
            "\tmargin-top: 15px;",
            "\tmargin-bottom: 5px;",
            "}",
            "",
            ".comment img {",
            "\twidth: 32px;",
            "\theight: 32px;",
            "}",
            "",
            ".comment h3 {",
            "\tfont-size: 1.3rem;",
            "\tmargin-bottom: 1.1rem;",
            "\tcolor: #113;",
            "}",
            "",
            ".comment section {",
            "\tfont-size: 1.1rem;",
            "}"
        ]
    },

    "Monthly Calendar Example Basic Styles": {
        "prefix": "ld-gc-ae-monthly-calendar-styles",
        "body": [
            "#monthly-calendar {",
            "\tbackground: #FFF;",
            "}",
            "",
            "#monthly-calendar > header {",
            "\tdisplay: flex;",
            "\talign-items: center;",
            "\tjustify-content: center;",
            "\tbackground: #000;",
            "\tcolor: #FFF;",
            "}",
            "",
            "#monthly-calendar > header > h1 {",
            "\tfont-size: 4vmin;",
            "}",
            "",
            ".day-header-field {",
            "\tdisplay: flex;",
            "\tjustify-content: center;",
            "\talign-items: center;",
            "\tfont-size: 2vmin;",
            "\tfont-weight: bold;",
            "}",
            "",
            ".day-field {",
            "\tfont-size: 2vmin;",
            "\tdisplay: flex;",
            "\tjustify-content: center;",
            "\talign-items: center;",
            "\tborder: 1px solid #BBB;",
            "\tbackground: #DDD;",
            "}",
            "",
            ".not-current-month {",
            "\tbackground: transparent;",
            "}"
        ]
    },
    
    "Movie Gallery Mobile Shell CSS": {
        "prefix": "ld-gc-ae-movie-gallery-mobile-shell-styles",
        "body": [
            "body {",
            "\tdisplay: flex;",
            "}",
            "",
            "body > header {",
            "\tposition: fixed;",
            "\tleft: 0;",
            "\ttop: 0;",
            "\tright: 0;",
            "\theight: 14vw;",
            "\tdisplay: flex;",
            "\talign-items: center;",
            "\tborder-bottom: 1px solid #FFF;",
            "\tpadding-top: 4vw;",
            "\tpadding-left: 4vw;",
            "\tpadding-right: 4vw;",
            "}",
            "",
            "h1 {",
            "\twhite-space: nowrap;",
            "\toverflow: hidden;",
            "\ttext-overflow: ellipsis;",
            "\tfont-size: 6vw;",
            "\tmargin-bottom: 4vw;",
            "}",
            "",
            "body > main {",
            "\tflex: 1 0 0;",
            "\tbackground: #111;",
            "\tdisplay: flex;",
            "\tpadding: 0 4vw;",
            "\tmargin-top: 14vw;",
            "}"
        ]
    },

    "Movie Gallery Article CSS": {
        "prefix": "ld-gc-ae-movie-gallery-article-styles",
        "body": [
            "#gallery {",
            "\tpadding: 4vw 0;",
            "}",
            "",
            "#gallery > article {",
            "\tbackground-repeat: no-repeat;",
            "\tbackground-size: cover;",
            "\tbackground-position: center;",
            "\tposition: relative;",
            "\twidth: 92vw;",
            "\theight: 100%;",
            "}",
            "",
            ".details {",
            "\theight: 30vw;",
            "\tpadding: 2vw 2vw;",
            "\tbackground: rgba(0, 0, 0, 0.4);",
            "}",
            "",
            "#gallery > article h2 {",
            "\tmargin-bottom: 1vw;",
            "\tfont-size: 10vw;",
            "}",
            "",
            "#gallery > article p {",
            "\tfont-size: 4vw;",
            "}"
        ]
    },

    "Movie Gallery Remaining CSS for Single Row Layout": {
        "prefix": "ld-gc-ae-movie-gallery-remaining-styles-for-single-row-layout",
        "body": [
            "::-webkit-scrollbar {",
            "\tdisplay: none;",
            "}",
            "",
            "@media (orientation: landscape) {",
            "\tbody > header {",
            "\t\tpadding-top: 3vh;",
            "\t\tpadding-left: 2vw;",
            "\t\tpadding-right: 2vw;",
            "\t\theight: 10vh;",
            "\t}",
            "",
            "\th1 {",
            "\t\tfont-size: 4vh;",
            "\t\tmargin-bottom: 3vh;",
            "\t}",
            "",
            "\tbody > main {",
            "\t\tmargin-top: 10vh;",
            "\t\tpadding: 0 2vw;",
            "\t}",
            "",
            "\t#gallery {",
            "\t\tpadding: 4vh 0;",
            "\t}",
            "",
            "\t#gallery > article {",
            "\t\tbackground-size: 70vh 92vh;",
            "\t\tbackground-position: left top;",
            "\t}",
            "",
            "\t.details {",
            "\t\tmargin-left: 75vh;",
            "\t\theight: 100%;",
            "\t\tpadding: 2vh 2vh;",
            "\t\tpadding-top: 0;",
            "\t\tbackground: transparent;",
            "\t}",
            "",
            "\t#gallery > article h2 {",
            "\t\tmargin-bottom: 2vh;",
            "\t\tfont-size: 7vh;",
            "\t}",
            "",
            "\t#gallery > article p {",
            "\t\tfont-size: 4vh;",
            "\t}",
            "}",
            "",
            "@media (orientation: portrait) and (min-width: 768px) {",
            "\tbody > header {",
            "\t\tpadding-top: 3vw;",
            "\t\tpadding-left: 4vw;",
            "\t\tpadding-right: 4vw;",
            "\t}",
            "",
            "\th1 {",
            "\t\tfont-size: 4vw;",
            "\t\tmargin-bottom: 3vw;",
            "\t}",
            "",
            "\t.details {",
            "\t\tpadding: 2vw 2vw;",
            "\t\theight: 20vw;",
            "\t}",
            "",
            "\t#gallery > article h2 {",
            "\t\tfont-size: 5vw;",
            "\t}",
            "",
            "\t#gallery > article p {",
            "\t\tfont-size: 3vw;",
            "\t}",
            "}"
        ]
    },

    "Movie Gallery iPad Grid Layout": {
        "prefix": "ld-gc-ae-ipad-grid-layout",
        "body": [
            "@media (orientation: landscape) and (min-width: 1024px) {",
            "\th1 {",
            "\t\tfont-size: 3vh;",
            "\t}",
            "",
            "\t#gallery {",
            "\t\tpadding: 4vh 0;",
            "\t}",
            "",
            "\t#gallery > article {",
            "\t\tbackground-size: cover;",
            "\t\theight: 100%;",
            "\t\twidth: 100%;",
            "\t\tpadding: 0;",
            "\t}",
            "",
            "\t.details {",
            "\t\tmargin-left: 0;",
            "\t\tpadding-top: 2vh;",
            "\t\theight: 14vh;",
            "\t\tbackground: rgba(0, 0, 0, 0.4);",
            "\t}",
            "",
            "\t#gallery > article h2 {",
            "\t\tmargin-bottom: 1vh;",
            "\t\tfont-size: 2.7vh;",
            "\t}",
            "",
            "\t#gallery > article p {",
            "\t\tfont-size: 1.4vh;",
            "\t}",
            "",
            "\t#gallery > article.feature-movie {",
            "\t\tbackground-position: center center;",
            "\t}",
            "}"
        ]
    }
}