{
    "Holy Grail Example Basic Styles": {
        "prefix": "ld-gc-ae-holy-grail-styles",
        "body": [
            "h1 {",
            "\tfont-size: 1.6rem;",
            "\tfont-weight: normal;",
            "}",
            "",
            "#holygrail-container {",
            "\tbackground: #FFF;",
            "\theight: 100%;",
            "\tmin-width: 400px;",
            "\tmin-height: 270px;",
            "}",
            "",
            "header, footer, nav, aside, section {",
            "\tpadding: 20px;",
            "\tborder: 1px solid #000;",
            "\ttext-align: center;",
            "}"
        ]
    },

    "Media Objects Layout Basic Styles": {
        "prefix":"ld-gc-ae-media-objects-layout-styles",
        "body": [
            ".media {",
            "\tbackground: #0048AA;",
            "\tcolor: #FFF;",
            "\tpadding: 15px;",
            "\tmargin-bottom: 40px;",
            "\tbox-sizing: border-box;",
            "}",
            "",
            ".media img {",
            "\tborder-radius: 100%;",
            "\twidth: 48px;",
            "\theight: 48px;",
            "}",
            "",
            ".media h2 {",
            "\tfont-size: 1.6rem;",
            "\tmargin-bottom: 1.3rem;",
            "\tcolor: #FFF;",
            "}",
            "",
            ".media section {",
            "\tfont-size: 1.3rem;",
            "}",
            "",
            ".media section p {",
            "\tmargin-bottom: 0.6rem;",
            "}",
            "",
            ".comment {",
            "\tbackground: #CCDDFF;",
            "\tpadding: 7px;",
            "\tcolor: #131;",
            "\tmargin-top: 15px;",
            "\tmargin-bottom: 5px;",
            "}",
            "",
            ".comment img {",
            "\twidth: 32px;",
            "\theight: 32px;",
            "}",
            "",
            ".comment h3 {",
            "\tfont-size: 1.3rem;",
            "\tmargin-bottom: 1.1rem;",
            "\tcolor: #113;",
            "}",
            "",
            ".comment section {",
            "\tfont-size: 1.1rem;",
            "}"
        ]
    },

    "Monthly Calendar Example Basic Styles": {
        "prefix": "ld-gc-ae-monthly-calendar-styles",
        "body": [
            "#monthly-calendar {",
            "\tbackground: #FFF;",
            "}",
            "",
            "#monthly-calendar > header {",
            "\tdisplay: flex;",
            "\talign-items: center;",
            "\tjustify-content: center;",
            "\tbackground: #000;",
            "\tcolor: #FFF;",
            "}",
            "",
            "#monthly-calendar > header > h1 {",
            "\tfont-size: 4vmin;",
            "}",
            "",
            ".day-header-field {",
            "\tdisplay: flex;",
            "\tjustify-content: center;",
            "\talign-items: center;",
            "\tfont-size: 2vmin;",
            "\tfont-weight: bold;",
            "}",
            "",
            ".day-field {",
            "\tfont-size: 2vmin;",
            "\tdisplay: flex;",
            "\tjustify-content: center;",
            "\talign-items: center;",
            "\tborder: 1px solid #BBB;",
            "\tbackground: #DDD;",
            "}",
            "",
            ".not-current-month {",
            "\tbackground: transparent;",
            "}"
        ]
    }
}