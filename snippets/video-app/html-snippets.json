    "YouTube Clone Comment Heading Script": {
        "prefix": "ld-gc-app-ytc-comment-heading-script",
        "body": [
            "<script>",
            "\tconst downTickButton = document.getElementById('comment-downtick');",
            "\tconst upTickButton = document.getElementById('comment-uptick');",
            "\tconst commentListContainer = document.getElementById('comment-list-container');",
            "",
            "\tdownTickButton.addEventListener('click', () => {",
            "\t\tdownTickButton.style.display = 'none';",
            "\t\tupTickButton.style.display = 'block';",
            "\t\tcommentListContainer.style.display = 'block';",
            "\t});",
            "",
            "\tupTickButton.addEventListener('click', () => {",
            "\t\tdownTickButton.style.display = 'block';",
            "\t\tupTickButton.style.display = 'none';",
            "\t\tcommentListContainer.style.display = 'none';",
            "\t});",
            "</script>"
        ]
    },

