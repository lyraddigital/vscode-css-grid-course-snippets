{
	"YouTube Clone Top Bar CSS": {
        "prefix": "ld-gc-app-ytc-top-bar-styles",
        "body": [
            "h1 {",
            "\tfont-size: 1.5rem;",
            "\tfont-weight: normal;",
            "}",
            "",
            "body > header {",
            "\tpadding: 0 8px;",
            "\tbackground: #1a599c;",
            "\tcolor: #fff;",
            "}",
            "",
            "body > header > a {",
            "\twidth: 32px;",
            "\theight: 32px;",
            "}",
            "",
            "#search-videos-icon,",
            "#settings-icon {",
            "\tpadding-left: 4px;",
            "\twidth: 28px;",
            "}"
        ]
    },

    "YouTube Clone Thumbnail Image CSS": {
        "prefix": "ld-gc-app-ytc-thumbnail-styles",
        "body": [
            "#video-thumbnail > img {",
            "\twidth: 100%;",
            "\theight: 100%;",
            "}"
        ]
    },

    "YouTube Clone Video Title CSS": {
        "prefix": "ld-gc-app-ytc-video-title-styles",
        "body": [
            "h2 {",
            "\tfont-size: 1.8rem;",
            "\tline-height: 2.25rem;",
            "\tmargin-bottom: 3px;",
            "\tfont-weight: normal;",
            "}",
            "",
            "#video-details > header {",
            "\tpadding-left: 12px;",
            "\tpadding-right: 12px;",
            "}",
            "",
            "#video-views-count {",
            "\tfont-size: 1.2rem;",
            "\tcolor: rgba(17, 17, 17, 0.6);",
            "}"
        ]
    },

    "YouTube Clone Social Icons CSS": {
        "prefix": "ld-gc-app-ytc-social-icon-styles",
        "body": [
            "#social-container {",
            "\tpadding: 12px 0;",
            "}",
            "",
            ".social-item {",
            "\tcolor: rgba(17, 17, 17, 0.6);",
            "}",
            "",
            ".social-item.active {",
            "\tcolor: #1a599c;",
            "}",
            "",
            ".social-item > img {",
            "\twidth: 24px;",
            "\theight: 24px;",
            "\tcursor: pointer;",
            "}",
            "",
            ".social-item > p {",
            "\tfont-size: 1.2rem;",
            "}"
        ]
    },

    "YouTube Clone Channel Details CSS": {
        "prefix": "ld-gc-app-ytc-channel-detail-styles",
        "body": [
            "h3 {",
            "\tfont-size: 1.4rem;",
            "\tline-height: 1.75rem;",
            "\tmargin-bottom: 3px;",
            "}",
            "",
            "a {",
            "\ttext-decoration: none;",
            "\tcolor: #111;",
            "}",
            "",
            "#channel-details {",
            "\tpadding: 12px 0;",
            "\tborder-top: 1px solid rgba(136, 136, 136, 0.4);",
            "\tborder-bottom: 1px solid rgba(136, 136, 136, 0.4);",
            "}",
            "",
            "#channel-details img {",
            "\twidth: 34px;",
            "\theight: 34px;",
            "}",
            "",
            "#channel-subscriber-count {",
            "\tcolor: rgba(17, 17, 17, 0.6);",
            "\tfont-size: 1.2rem;",
            "}",
            "",
            "#channel-details button {",
            "\tdisplay: flex;",
            "\tjustify-content: flex-end;",
            "\talign-items: center;",
            "\tpadding-right: 8px;",
            "\toutline: none;",
            "\tborder: none;",
            "\tbackground: transparent;",
            "\tfont-size: 1.4rem;",
            "\tcolor: #1a599c;",
            "\ttext-transform: uppercase;",
            "\toutline: none;",
            "}",
            "",
            "#channel-details button > img {",
            "\theight: 16px;",
            "}"
        ]
    },

    "YouTube Clone Autoplay CSS": {
        "prefix": "ld-gc-app-ytc-autoplay-styles",
        "body": [
            "#related-videos > header {",
            "\tpadding-top: 16px;",
            "\tpadding-bottom: 16px;",
            "\tfont-size: 1.4rem;",
            "}",
            "",
            "#related-videos > header button {",
            "\twidth: 37px;",
            "\theight: 14px;",
            "\tborder: 0;",
            "\toutline: none;",
            "\tbackground: transparent;",
            "\tposition: relative;",
            "}",
            "",
            ".autoplay-slider {",
            "\tdisplay: block;",
            "\twidth: 37px;",
            "\theight: 14px;",
            "\tborder: 7px solid rgba(26,89,156, 0.38);",
            "\tborder-radius: 7px;",
            "}",
            "",
            ".autoplay-slider-toggle-button {",
            "\tdisplay: block;",
            "\twidth: 20px;",
            "\theight: 20px;",
            "\tposition: absolute;",
            "\ttop: -3px;",
            "\tborder-radius: 100%;",
            "\tbackground: white;",
            "\tbox-shadow: 0 1px 3px rgba(0,0,0,0.4);",
            "}"
        ]
    },

    "YouTube Clone Related Video List CSS": {
        "prefix": "ld-gc-app-ytc-related-video-list-styles",
        "body": [
            "#related-videos > ul > li {",
            "\tmargin-bottom: 12px;",
            "\tlist-style: none;",
            "}",
            "",
            "#related-videos > ul > li:last-of-type {",
            "\tmargin-bottom: 0;",
            "}",
            "",
            "#related-videos > ul > li article {",
            "\tmin-height: 90px;",
            "\tmax-height: 90px;",
            "\toverflow-y: hidden;",
            "}",
            "",
            "#related-videos > ul > li img {",
            "\theight: 100%;",
            "\twidth: 100%;",
            "}",
            "",
            "#related-videos > ul > li h4 {",
            "\tmax-height: 3.75em;",
            "\tdisplay: -webkit-box;",
            "\t-webkit-line-clamp: 3;",
            "\t-webkit-box-orient: vertical;",
            "\toverflow: hidden;",
            "\ttext-overflow: ellipsis;",
            "}",
            "",
            "#related-videos > ul > li p {",
            "\tfont-size: 1.2rem;",
            "\tcolor: rgba(17, 17, 17, 0.6);",
            "}"
        ]
    },

    "YouTube Clone Comment Heading CSS": {
        "prefix": "ld-gc-app-ytc-comment-heading-styles",
        "body": [
            "#video-comments h2 {",
            "\tfont-weight: bold;",            
            "}",
            "",
            "#comment-uptick,",
            "#comment-list-container {",
            "\tdisplay: none;",            
            "}"
        ]
    },

    "YouTube Clone Comment Form CSS": {
        "prefix": "ld-gc-app-ytc-comment-form-styles",
        "body": [
            "#comment-form {",
            "\tmargin-top: 16px;",
            "\tpadding: 12px;",
            "\tborder-bottom: 1px solid rgba(136, 136, 136, 0.4);",            
            "}",
            "",
            "#comment-form img {",
            "\twidth: 48px;",
            "\theight: 48px;",
            "\tborder-radius: 100%;",            
            "}",
            "",
            "#comment-form textarea {",
            "\tpadding: 6px;",
            "\toutline: none;",
            "\tcolor: #111;",
            "\tfont-size: 1.4rem;",
            "\tresize: vertical;",
            "\tborder: 1px solid rgba(135, 135, 135, 0.4);",
            "}",
            "",
            "#comment-form textarea::placeholder {",
            "\tcolor: rgba(135, 135, 135, 0.4);",
            "}"
        ]
    },

    "YouTube Clone Individual Comment CSS": {
        "prefix": "ld-gc-app-ytc-comment-styles",
        "body": [
            ".comment {",
            "\tpadding: 12px;",
            "\tfont-size: 1.1rem;",
            "\tcolor: rgba(17,17,17, 0.6);", 
            "\tborder-bottom: 1px solid rgba(135, 135, 135, 0.4);", 
            "}",
            "",
            ".comment:last-of-type {",
            "\tborder-bottom: none;",          
            "}",
            "",
            ".comment > a {",
            "\tfont-size: 1.4rem;",
            "\tcolor: #1a599c;",
            "}",
            "",
            ".my-comment > a > span {",
            "\tbackground: #1a599c;",
            "\tcolor: #fff;",
            "\tborder-radius: 8px;",
            "\tpadding: 1px 6px;",
            "\tdisplay: inline-block;",
            "}",
            "",
            ".comment > p {",
            "\tcolor: #000;",
            "\tfont-size: 1.3rem;",
            "\tmargin: 4px 0;",
            "}",
            "",
            ".comment-status-separator {",
            "\tfont-size: 2rem;",
            "\tline-height: 1.1rem;",
            "}",
            "",
            ".comment-statistics > img {",
            "\tmargin-left: 8px;",
            "\twidth: 11px;",
            "\theight: 10px;",
            "}"
        ]
    },

    "YouTube Clone Response Button CSS": {
        "prefix": "ld-gc-app-ytc-response-button-styles",
        "body": [
            ".see-response-container > button {",
            "\tbackground: transparent;",
            "\tborder: none;",
            "\ttext-transform: uppercase;",
            "\toutline: none;",
            "\tfont-size: 1.4rem;",
            "\tcolor: #1a599c;",
            "\tcursor: pointer;",
            "\tpadding: 10px 8px;",
            "}",
            "",
            ".response-list {",
            "\tdisplay: none;",
            "}"
        ]
    },

    "YouTube Clone Response CSS": {
        "prefix": "ld-gc-app-ytc-response-styles",
        "body": [
            ".response {",
            "\tmargin-bottom: 15px;",
            "\tpadding: 0;",
            "\tborder-bottom: 0;",
            "}",
            "",
            ".response > img {",
            "\twidth: 26px;",
            "\theight: 26px;",
            "}"
        ]
    },

    "YouTube Clone Related Videos Mobile Landscape CSS": {
        "prefix": "ld-gc-app-ytc-related-videos-mobile-landscape-styles",
        "body": [
            "@media (min-width: 550px) {",
            "\t#related-videos > ul {",
            "\t\tpadding-left: 8px;",
            "\t\tpadding-right: 8px;",
            "\t}",
            "",
            "\t#related-videos > ul > li article {",
            "\t\tmin-height: inherit;",
            "\t\tmax-height: inherit;",
            "\t}",
            "",
            "\t#related-videos > ul > li img {",
            "\t\theight: 26.6vw;",
            "\t\tmin-height: 141px;",
            "\t\tmax-height: 202px;",
            "\t}",
            "",
            "\t.related-video-details h4 {",
            "\t\tpadding-top: 4px;",
            "\t}",
            "}"
        ]
    },

    "YouTube Clone Related Videos Tablet Portrait CSS": {
        "prefix": "ld-gc-app-ytc-related-videos-tablet-portrait-styles",
        "body": [
            "@media (min-width: 768px) {",
            "\t#related-videos > ul > li img {",
            "\t\theight: 17.5vw;",
            "\t\tmin-height: 131px;",
            "\t\tmax-height: inherit;",
            "\t}",
            "}"
        ]
    },

    "YouTube Clone Related Videos Tablet Landscape CSS": {
        "prefix": "ld-gc-app-ytc-related-videos-tablet-landscape-styles",
        "body": [
            "@media (min-width: 800px) and (orientation: landscape) {",
            "\t#related-videos {",
            "\t\tpadding-left: 8px;",
            "\t\tpadding-right: 8px;",
            "\t}",
            "",
            "\t#related-videos > ul {",
            "\t\tpadding-left: 0;",
            "\t\tpadding-right: 0;",  
            "\t}",
            "",
            "\t#related-videos > ul > li img {",
            "\t\theight: 135px;",
            "\t\tmin-height: inherit;",
            "\t\tmax-height: inherit;",   
            "\t}",
            "}"
        ]
    },

    "YouTube Clone Related Videos Large Device Landscape CSS": {
        "prefix": "ld-gc-app-ytc-related-videos-large-landscape-styles",
        "body": [
            "@media (min-width: 1280px) and (orientation: landscape) {",
            "",
            "}"
        ]
    }
}