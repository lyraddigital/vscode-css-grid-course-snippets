            "}",
            "",
            "#straya .thumbnail-image {",
            "\tbackground-image: url(../images/stores/straya.png);",
            "}",
            "",
            "#snags .thumbnail-image {",
            "\tbackground-image: url(../images/stores/snags.png);",
            "}",
            "",
            "#oi .thumbnail-image {",
            "\tbackground-image: url(../images/stores/oi.png);",
            "}",
            "",
            "#chips-mate .thumbnail-image {",
            "\tbackground-image: url(../images/stores/chips-mate.png);",
            "}",
            "",
            "#fridee-nights .thumbnail-image {",
            "\tbackground-image: url(../images/stores/fridee-nights.png);",
            "}",
            "",
            "#maccas .thumbnail-image {",
            "\tbackground-image: url(../images/stores/maccas.png);",
            "}"
        ]
    },

    "Uber Eats Clone Footer CSS": {
        "prefix": "ld-gc-app-uec-footer-styles",
        "body": [
            "hr {",
            "\tmargin: 24px 0;",
            "\tborder-bottom: 1px solid #262;",
            "}",
            "",
            "body > footer li {",
            "\tlist-style: none;",
            "}",
            "",
            "body > footer a {",
            "\tdisplay: block;",
            "\tcolor: #007340;",
            "\ttext-decoration: none;",
            "\tmargin-bottom: 12px;",
            "}",
            "",
            "body > footer a:hover {",
            "\tcolor: #638a78;",
            "}",
            "",
            "#social-media-links {",
            "\tdisplay: flex;",
            "\theight: 32px;",
            "\tmargin-bottom: 12px;",
            "}",
            "",
            "#social-media-links img {",
            "\twidth: 32px;",
            "}",
            "",
            "#security-section {",
            "\tfont-size: 1.2rem;",
            "}",
            "",
            "#security-section > small {",
            "\tdisplay: block;",
            "\tmargin-bottom: 12px;",
            "}"
        ]
    },

