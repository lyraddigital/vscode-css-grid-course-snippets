{
    "Uber Eats Clone Main Layout CSS": {
        "prefix": "ld-gc-app-uec-main-layout-styles",
        "body": [
            "body > header,",
            "body > main,",
            "body > footer {",
            "\tpadding: 0 16px;",
            "}",
            "",
            "body > header {",
            "\tbackground: #007340;",
            "\tbox-shadow: 0 20px 50px -20px #007340;",
            "\tz-index: 1;",
            "}",
            "",
            "body > main {",
            "\tbackground: #FFFFEF;",
            "}",
            "",
            "body > footer {",
            "\tbackground: #FD0;",
            "}"
        ]
    },

    "Uber Eats Clone Top Header CSS": {
        "prefix": "ld-gc-app-uec-top-header-styles",
        "body": [
            "body > header > a {",
            "\tdisplay: flex;",
            "\talign-items: center;",
            "}",
            "",
            "body > header > a > img {",
            "\theight: 30px;",
            "}",
            "",
            "body > header > img {",
            "\tborder-radius: 100%;",
            "\tcursor: pointer;",
            "}"
        ]
    }, 

    "Uber Eats Clone Store Search CSS": {
        "prefix": "ld-gc-app-uec-store-search-styles",
        "body": [
            "#store-search-container > form {",
            "\tborder-bottom: 1px solid #8A8;",
            "}",
            "",
            "#store-search-container > form > img {",
            "\twidth: 30px;",
            "\theight: 30px;",
            "}",
            "",
            "#store-search-container input {",
            "\tborder: none;",
            "\toutline: none;",
            "\tpadding: 8px;",
            "\tfont-size: 2rem;",
            "\tfont-weight: bold;",
            "\tline-height: 2.4rem;",
            "\tbackground: transparent;",
            "\tcolor: #262;",
            "}",
            "",
            "#store-search-container input::placeholder {",
            "\tcolor: #ACA;",
            "\tfont-weight: normal;",
            "}"
        ]
    },

    "Uber Eats Clone Main Delivery CSS": {
        "prefix": "ld-gc-app-uec-main-delivery-styles",
        "body": [
            "#main-delivery-container {",
            "\tpadding-top: 16px;",
            "}",
            "",
            ".main-delivery-label {",
            "\tcolor: #535331;",
            "\tfont-weight: 400;",
            "}",
            "",
            ".main-delivery-choice {",
            "\tborder: 1px solid #262;",
            "\tpadding: 8px;",
            "}"
        ]
    },

    "Uber Eats Clone Store List CSS": {
        "prefix": "ld-gc-app-uec-store-list-styles",
        "body": [
            "h2 {",
            "\tfont-size: 2.2rem;",
            "\tfont-weight: 400;",
            "}",
            "",
            "#store-list-container > h2 {",
            "\tmargin-top: 16px;",
            "\tmargin-bottom: 32px;",
            "}",
            "",
            "#store-list-container > ul {",
            "\tlist-style: none;",
            "}",
            "",
            "#store-list-container > ul a {",
            "\ttext-decoration: none;",
            "\tcolor: #262626;",
            "}",
            "",
            "#store-list-container li article {",
            "\tbackground: #fd6;",
            "\tpadding-bottom: 20px;",
            "}",
            "",
            "#store-list-container li article .thumbnail-image {",
            "\tbackground-repeat: no-repeat;",
            "\tbackground-size: cover;",
            "\tbackground-position: center center;",
            "\theight: 200px;",
            "}",
            "",
            "#store-list-container li article h3 {",
            "\tcolor: #007340;",
            "\tfont-size: 1.6rem;",
            "\tfont-weight: 400;",
            "\tmargin-top: 10px;",
            "\tpadding: 0 10px;",
            "\twhite-space: nowrap;",
            "\toverflow-x: hidden;",
            "\ttext-overflow: ellipsis;",
            "}",
            "",
            "#store-list-container li article p {",
            "\tfont-size: 1.4rem;",
            "\tpadding: 0 10px;",
            "}",
            "",
            "#store-list-container li article p:first-of-type {",
            "\tcolor: #252;",
            "\twhite-space: nowrap;",
            "\toverflow-x: hidden;",
            "\ttext-overflow: ellipsis;",
            "}",
            "",
            "#store-list-container li article p:last-of-type {",
            "\tmargin-top: 8px;",
            "}",
            "",
            "#store-list-container li article p:last-of-type > span {",
            "\tpadding: 4px 8px;",
