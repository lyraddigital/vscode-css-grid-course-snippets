    "Uber Eats Clone Very Large Device CSS": {
        "prefix": "ld-gc-app-uec-very-large-device-styles",
        "body": [
            "@media (min-width: 1040px) {",
            "\t#delivery-widget-container {",
            "\t\tfont-size: 1.4rem;",
            "\t\tcolor: #FD0;",
            "\t}",
            "",
            "\t#delivery-widget-time {",
            "\t\tdisplay: flex;",
            "\t\talign-items: center;",
            "\t\tborder: 1px solid #FD0;",
            "\t\tpadding: 0 12px;",
            "\t}",
            "",
            "\t#delivery-widget-label {",
            "\t\tdisplay: flex;",
            "\t\talign-items: center;",
            "\t}",
            "",
            "\t#delivery-widget-address {",
            "\t\tdisplay: flex;",
            "\t\tposition: relative;",
            "\t}",
            "",
            "\t#delivery-widget-address > img {",
            "\t\tposition: absolute;",
            "\t\tleft: 16px;",
            "\t\ttop: 12px;",
            "\t\twidth: 16px;",
            "\t\theight: 16px;",
            "\t}",
            "",
            "\t#delivery-widget-address > input {",
            "\t\tflex: 1 0 0%;",
            "\t\tpadding-left: 48px;",
            "\t\tpadding-right: 16px;",
            "\t\tfont-size: 1.4rem;",
            "\t\tcolor: #262;",
            "\t\tbackground: #EFE;",
            "\t\toutline: none;",
            "\t\tborder: 2px solid #FD0;",
            "\t\tborder-radius: 4px;",
            "\t}",
            "}"
